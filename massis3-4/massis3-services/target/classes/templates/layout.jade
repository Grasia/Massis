//==================
mixin boxinfo1(uptext, downtext)
	.col-md-3.col-sm-12.col-xs-12
		.panel.panel-primary.text-center.no-boder.bg-color-green.green
			.panel-left.pull-left.green
				i.fa.fa-eye.fa-5x
			.panel-right
				h3= uptext
				strong= downtext

mixin linechart1(chartTitle)
	.col-md-7
		.panel.panel-default
			.panel-heading
				=chartTitle
			.panel-body
				#morris-line-chart



